<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Windsurfer Tracker</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Windsurfer Tracker</string>
	<key>CFBundlePackageType</key>
	<string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>WKApplication</key>
	<true/>
	<key>WKCompanionAppBundleIdentifier</key>
	<string>nz.co.tracker.windsurfer</string>
	<key>WKRunsIndependentlyOfCompanionApp</key>
	<true/>
	<key>WKWatchOnly</key>
	<false/>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Windsurfer Tracker uses GPS to calculate distance traveled and speed during your sailing workouts.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Windsurfer Tracker needs continuous location access to track distance and speed throughout your sailing workout.</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
	</array>
	<key>WKBackgroundModes</key>
	<array>
		<string>workout-processing</string>
	</array>
	<key>NSHealthShareUsageDescription</key>
	<string>Windsurfer Tracker reads your heart rate to display during sailing workouts and track your fitness over time.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>Windsurfer Tracker records your sailing workouts including distance traveled, heart rate, and workout duration to Apple Health.</string>
</dict>
</plist>
